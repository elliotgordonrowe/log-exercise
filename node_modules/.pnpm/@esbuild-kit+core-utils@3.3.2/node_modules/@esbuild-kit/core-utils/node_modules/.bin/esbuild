#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/esbuild@0.18.20/node_modules/esbuild/bin/node_modules:/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/esbuild@0.18.20/node_modules/esbuild/node_modules:/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/esbuild@0.18.20/node_modules:/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/esbuild@0.18.20/node_modules/esbuild/bin/node_modules:/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/esbuild@0.18.20/node_modules/esbuild/node_modules:/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/esbuild@0.18.20/node_modules:/home/pierre/Desktop/circularise/log-exercise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../../esbuild@0.18.20/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
